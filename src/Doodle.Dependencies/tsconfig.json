{
    "compilerOptions": {
      "alwaysStrict": false,
      "noImplicitAny": false,
      "noEmitOnError": true,
      "removeComments": true,
      "sourceMap": false,
      "target": "ES2020",
      "module": "ES2020",
      "moduleResolution": "Node"
    },
    "compileOnSave": true,
    "include": [
      "npm/src"
    ],
    "exclude": [
      "npm/node_modules", 
      "wwwroot"
    ]
  }
  